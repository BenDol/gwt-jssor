                <g:HTMLPanel ui:field="loading" />
            </div>
            <g:HTMLPanel ui:field="slides"/>
            <!-- Thumbnail Navigator -->
            <div data-u="thumbnavigator" class="jssort01" style="position:absolute;left:0px;bottom:0px;height:100px;" data-autocenter="1">
                <!-- Thumbnail Item Skin Begin -->
                <div data-u="slides" style="cursor: default;">
                    <div data-u="prototype" class="p">
                        <div class="w">
                            <div data-u="thumbnailtemplate" class="t" />
                        </div>
                        <div class="c" />
                    </div>
                </div>
                <!-- Thumbnail Item Skin End -->
